# Optional defaults used by vibebox templates and scripts.
# Copy this file to .env and set only the values you need.

VIBEBOX_CONTAINER_USERNAME=vibebox
VIBEBOX_DEVCONTAINER_IMAGE=vibebox/your-repo:latest

# Enable ECR login at devcontainer initialize step.
VIBEBOX_ENABLE_ECR_LOGIN=false
VIBEBOX_ENVCHAIN_SERVICE=vibebox:your-repo:ecr
VIBEBOX_ECR_REGISTRY=
VIBEBOX_AWS_REGION=

# Optional docker build argument.
VIBEBOX_S3_DEV_BUCKET_NAME=

# Optional bootstrap repos (comma-separated, e.g. org/tools,org/dotfiles).
VIBEBOX_BOOTSTRAP_REPOS=

# Enable optional dotfiles installation.
VIBEBOX_ENABLE_DOTFILES_SETUP=false
